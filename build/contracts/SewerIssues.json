  "source": "pragma solidity >=0.4.22 <0.9.0;\n\ncontract SewerIssues {\n    uint public issueCount = 0;\n\n    struct Issue {\n        uint id;\n        string permInfo;\n    }\n\n    mapping(uint => Issue) public issues;\n\n    function createIssue(string memory _permInfo) public {\n        issueCount ++;\n        issues[issueCount] = Issue(\n            issueCount,\n            _permInfo\n        );\n    }\n}",
